{
    "name": "PiPiGui on PC",
    "description": "For yayayayayayayayaya",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [{
        "run_at": "document_start",
        "js": ["contentscript.js"],
        "matches": [
            "*://*.pipigui.cc/*"
        ]
    }],
    "icons": {
        "48": "icon48.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": {
            "48": "icon48.png"
        }
    }
}